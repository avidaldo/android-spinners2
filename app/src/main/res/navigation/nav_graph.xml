<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.example.spinners2.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_MainFragment_to_SpinnersFragment"
            app:destination="@id/SpinnersFragment" />
        <action
            android:id="@+id/action_MainFragment_to_autocompletarFragment"
            app:destination="@id/AutocompletarFragment" />
        <action
            android:id="@+id/action_MainFragment_to_ej01ListViewFragment"
            app:destination="@id/ej01ListViewFragment" />
    </fragment>

    <fragment
        android:id="@+id/SpinnersFragment"
        android:name="com.example.spinners2.spinners.SpinnersFragment"
        android:label="SpinnersFragment"
        tools:layout="@layout/fragment_spinners">
        <action
            android:id="@+id/action_SpinnersFragment_to_ej01EntriesFragment"
            app:destination="@id/ej01EntriesFragment" />
        <action
            android:id="@+id/action_SpinnersFragment_to_ej02OnItemSelectedFragment"
            app:destination="@id/ej02OnItemSelectedFragment" />
        <action
            android:id="@+id/action_SpinnersFragment_to_ej03FromArrayFragment"
            app:destination="@id/ej03FromArrayFragment" />
        <action
            android:id="@+id/action_SpinnersFragment_to_ej04FromResourceFragment"
            app:destination="@id/ej04FromResourceFragment" />
        <action
            android:id="@+id/action_SpinnersFragment_to_ej05FromArrayListFragment"
            app:destination="@id/ej05FromArrayListFragment" />
        <action
            android:id="@+id/action_SpinnersFragment_to_ej06Fragment"
            app:destination="@id/ej06Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ej01EntriesFragment"
        android:name="com.example.spinners2.spinners.Ej01EntriesFragment"
        android:label="Ej01EntriesFragment" />
    <fragment
        android:id="@+id/ej02OnItemSelectedFragment"
        android:name="com.example.spinners2.spinners.Ej02OnItemSelectedFragment"
        android:label="Ej02OnItemSelectedFragment" />
    <fragment
        android:id="@+id/ej03FromArrayFragment"
        android:name="com.example.spinners2.spinners.Ej03FromArrayFragment"
        android:label="Ej03FromArrayFragment" />
    <fragment
        android:id="@+id/ej04FromResourceFragment"
        android:name="com.example.spinners2.spinners.Ej04FromResourceFragment"
        android:label="Ej04FromResourceFragment" />
    <fragment
        android:id="@+id/ej05FromArrayListFragment"
        android:name="com.example.spinners2.spinners.Ej05FromArrayListFragment"
        android:label="Ej05FromArrayListFragment" />
    <fragment
        android:id="@+id/ej06Fragment"
        android:name="com.example.spinners2.spinners.Ej06DialogModeFragment"
        android:label="Ej06Fragment" />
    <fragment
        android:id="@+id/AutocompletarFragment"
        android:name="com.example.spinners2.autocompletar.AutocompletarFragment"
        android:label="fragment_autocompletar"
        tools:layout="@layout/fragment_autocompletar" >
        <action
            android:id="@+id/action_autocompletarFragment_to_ej01AutocompFragment"
            app:destination="@id/ej01AutocompFragment" />
        <action
            android:id="@+id/action_autocompletarFragment_to_ej02AutocompFragment"
            app:destination="@id/ej02aAutocompFragment" />
        <action
            android:id="@+id/action_autocompletarFragment_to_ej03AutocompFragment"
            app:destination="@id/ej02bAutocompFragment" />
    </fragment>
    <fragment
        android:id="@+id/ej01AutocompFragment"
        android:name="com.example.spinners2.autocompletar.Ej01AutocompFragment"
        android:label="Ej01AutocompFragment" />
    <fragment
        android:id="@+id/ej02aAutocompFragment"
        android:name="com.example.spinners2.autocompletar.Ej02aAutocompFragment"
        android:label="Ej02aAutocompFragment" />
    <fragment
        android:id="@+id/ej02bAutocompFragment"
        android:name="com.example.spinners2.autocompletar.Ej02bAutocompFragment"
        android:label="Ej02bAutocompFragment" />
    <fragment
        android:id="@+id/ej01ListViewFragment"
        android:name="com.example.spinners2.listviews.Ej01ListViewFragment"
        android:label="fragment_listview_ej01"
        tools:layout="@layout/fragment_listview_ej01" />
</navigation>